{
  "requirements": {
    "plugins": ["fast_prompt_tags"],
    "colors": "true_color",
    "fonts": ["FiraCode Nerd Font", "Hack Nerd Font", "JetBrains Mono", "Source Code Pro"]
  },
  "terminal_title": "{SHELL} {SHELL_VER} | {USERNAME}@{HOSTNAME}",
