    {
      "tag": "userseg",
      "content": " {USERNAME} ",
      "bg_color": "#1D2433",
      "fg_color": "#D7DAE0",
      "separator": "\uE0B0",
      "separator_fg": "#1D2433",
      "separator_bg": "#2E3C54",
      "forward_separator": "\uE0B6",
      "forward_separator_fg": "#1D2433",
      "forward_separator_bg": "RESET",
      "align": "left"
    },
    {
      "tag": "hostseg",
