{
  "requirements": {
    "colors": "true_color",
    "fonts": ["FiraCode Nerd Font", "Hack Nerd Font", "JetBrains Mono"]
  },
  "terminal_title": "{SHELL} {SHELL_VER} | {USERNAME}@{HOSTNAME}",
  "ps1_segments": [
    {
      "tag": "rainbow_user",
      "content": " {USERNAME}@{HOSTNAME} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ff0000, #ff7f00, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    },
    {
      "tag": "rainbow_path",
      "content": " {DIRECTORY} ",
      "bg_color": "RESET", 
      "fg_color": "gradient(#ffff00, #00ff00, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    },
    {
      "tag": "rainbow_prompt",
      "content": " \u276F ",
      "bg_color": "RESET",
      "fg_color": "gradient(#0000ff, #4b0082, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "git_segments": [
    {
      "tag": "git_path",
      "content": " {LOCAL_PATH} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ff69b4, #ff1493, horizontal)",
      "separator": " ",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "git_branch",
      "content": " \uE0A0 {GIT_BRANCH} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#32cd32, #228b22, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    },
    {
      "tag": "git_status",
      "content": " {GIT_STATUS} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ffa500, #ff6347, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "ai_segments": [
    {
      "tag": "ai_model",
      "content": " \uF4A4 {AI_MODEL} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#9370db, #8a2be2, horizontal)",
      "separator": " / ",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "ai_mode", 
      "content": " {AI_AGENT_TYPE} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#dc143c, #b22222, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "newline_segments": [
    {
      "tag": "newline_prompt",
      "content": " \u2192 ",
      "bg_color": "RESET",
      "fg_color": "gradient(#00ced1, #4682b4, horizontal)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "fill_char": " ",
  "fill_fg_color": "gradient(#696969, #778899, horizontal)",
  "fill_bg_color": "RESET"
}