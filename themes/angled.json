        {
            "type": "text",
            "content": " 📁 {DIRECTORY}  ",
            "bg_color": "P10K_DIR_BG",
            "fg_color": "P10K_DIR_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_DIR_BG",
            "separator_bg": "P10K_GIT_STATUS_BG"
