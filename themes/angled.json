{
    "use_segments": true,
    "terminal_title": "{SHELL}  {USERNAME}@{HOSTNAME} {TIME}  {DATE}",
    "use_newline": true,
    "ps1_segments": [
        {
            "type": "text",
            "content": "  {SHELL}  ",
            "bg_color": "P10K_SHELL_BG",
            "fg_color": "P10K_SHELL_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_SHELL_BG",
            "separator_bg": "P10K_USER_BG",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "P10K_SHELL_BG",
            "forward_separator_bg": "RESET"
        },
        {
            "type": "text",
            "content": " {USERNAME}  ",
            "bg_color": "P10K_USER_BG",
            "fg_color": "P10K_USER_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_USER_BG",
            "separator_bg": "P10K_HOST_BG"
        },
        {
            "type": "text",
            "content": " {HOSTNAME}  ",
            "bg_color": "P10K_HOST_BG",
            "fg_color": "P10K_HOST_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_HOST_BG",
            "separator_bg": "P10K_DIR_BG"
        },
        {
            "type": "text",
            "content": " 📁 {PATH}  ",
            "bg_color": "P10K_DIR_BG",
            "fg_color": "P10K_DIR_FG",
            "separator": "\uE0B0                           ",
            "separator_fg": "P10K_DIR_BG",
            "separator_bg": "RESET"
        },
        {
            "type": "text",
            "content": " {TIME} ",
            "bg_color": "P10K_TIME_BG",
            "fg_color": "P10K_TIME_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_TIME_BG",
            "separator_bg": "RESET",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "P10K_TIME_BG",
            "forward_separator_bg": "RESET"
        }
    ],
    "git_segments": [
        {
            "type": "text",
            "content": "  {USERNAME} ",
            "bg_color": "P10K_USER_BG",
            "fg_color": "P10K_USER_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_USER_BG",
            "separator_bg": "P10K_DIR_BG",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "P10K_USER_BG",
            "forward_separator_bg": "RESET"
