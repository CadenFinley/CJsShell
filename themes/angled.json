        {
            "type": "text",
            "content": " {GIT_BRANCH}",
            "bg_color": "P10K_GIT_STATUS_BG",
            "fg_color": "P10K_GIT_STATUS_FG",
            "separator": ""
        },
        {
            "type": "text",
            "content": "{GIT_STATUS}  ",
            "bg_color": "P10K_GIT_STATUS_BG",
            "fg_color": "P10K_GIT_STATUS_FG",
            "separator": "\uE0B0",
            "separator_fg": "P10K_GIT_STATUS_BG",
            "separator_bg": "RESET"
        }
