{
  "requirements": {
    "colors": "true_color",
    "fonts": ["FiraCode Nerd Font", "Hack Nerd Font", "JetBrains Mono", "Source Code Pro"]
  },
  "terminal_title": "{USERNAME}@{HOSTNAME}: {PATH}",
  "ps1_segments": [
    {
      "tag": "username",
      "content": " {USERNAME} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ff6b6b, #4ecdc4)",
      "separator": "@",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "hostname",
      "content": "{HOSTNAME}",
      "bg_color": "RESET",
      "fg_color": "gradient(#4ecdc4, #45b7d1)",
      "separator": ":",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "directory",
      "content": " {DIRECTORY} ",
      "bg_color": "gradient(#667eea, #764ba2)",
      "fg_color": "#FFFFFF",
      "separator": " $ ",
      "separator_fg": "gradient(#ffeaa7, #fab1a0)",
      "separator_bg": "RESET"
    }
  ],
  "git_segments": [
    {
      "tag": "path",
      "content": " {LOCAL_PATH} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#a8e6cf, #dcedc8)",
      "separator": " ",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "branch",
      "content": " \uE0A0 {GIT_BRANCH} ",
      "bg_color": "gradient(#ffd3a5, #fd9853)",
      "fg_color": "#2d3436",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    },
    {
      "tag": "status",
      "content": " {GIT_STATUS} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ff9a9e, #fad0c4)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "ai_segments": [
    {
      "tag": "model",
      "content": " \uF085 {AI_MODEL} ",
      "bg_color": "gradient(#667eea, #764ba2)",
      "fg_color": "#FFFFFF",
      "separator": " / ",
      "separator_fg": "#FFFFFF",
      "separator_bg": "RESET"
    },
    {
      "tag": "mode",
      "content": " {AI_AGENT_TYPE} ",
      "bg_color": "RESET",
      "fg_color": "gradient(#ff6b6b, #ee5a52)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "newline_segments": [
    {
      "tag": "prompt",
      "content": " \u276F ",
      "bg_color": "RESET",
      "fg_color": "gradient(#a8e6cf, #ff8a80)",
      "separator": "",
      "separator_fg": "RESET",
      "separator_bg": "RESET"
    }
  ],
  "fill_char": " ",
  "fill_fg_color": "gradient(#ffeaa7, #fab1a0)",
  "fill_bg_color": "RESET"
}