            "separator_fg": "PASTEL_TURQUOISE",
            "separator_bg": "RESET"
        },
        {
            "type": "text",
            "content": " {AI_DIVIDER}  ",
            "bg_color": "RESET",
            "fg_color": "PASTEL_PEACH",
            "separator": "\uE0B0",
            "separator_fg": "RESET",
