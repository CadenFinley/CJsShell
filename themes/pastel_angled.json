{
    "use_segments": true,
    "terminal_title": "{SHELL}  {USERNAME}@{HOSTNAME} {TIME}  {DATE}",
    "use_newline": true,
    "ps1_segments": [
        {
            "type": "text",
            "content": "  {SHELL}  ",
            "bg_color": "PASTEL_BLUE",
            "fg_color": "PASTEL_BLUSH",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_BLUE",
            "separator_bg": "PASTEL_LAVENDER",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "PASTEL_BLUE",
            "forward_separator_bg": "RESET"
        },
        {
            "type": "text",
            "content": " {USERNAME}  ",
            "bg_color": "PASTEL_LAVENDER",
            "fg_color": "PASTEL_BLUSH",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_LAVENDER",
            "separator_bg": "PASTEL_PURPLE"
        },
        {
            "type": "text",
            "content": " {HOSTNAME}  ",
            "bg_color": "PASTEL_PURPLE",
            "fg_color": "PASTEL_BLUSH",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_PURPLE",
            "separator_bg": "PASTEL_CYAN"
