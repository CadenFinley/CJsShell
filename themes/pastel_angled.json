        {
            "type": "text",
            "content": " {TIME} ",
            "bg_color": "PASTEL_SKY",
            "fg_color": "PASTEL_PURPLE",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_SKY",
            "separator_bg": "RESET",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "PASTEL_SKY",
            "forward_separator_bg": "RESET"
        }
    ],
    "git_segments": [
        {
            "type": "text",
            "content": "  {USERNAME} ",
            "bg_color": "PASTEL_LAVENDER",
            "fg_color": "PASTEL_BLUSH",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_LAVENDER",
            "separator_bg": "PASTEL_CYAN",
            "forward_separator": "\uE0B2",
            "forward_separator_fg": "PASTEL_LAVENDER",
