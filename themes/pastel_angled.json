        {
            "type": "text",
            "content": " {GIT_BRANCH}",
            "bg_color": "PASTEL_MINT",
            "fg_color": "PASTEL_PURPLE",
            "separator": ""
        },
        {
            "type": "text",
            "content": "{GIT_STATUS}  ",
            "bg_color": "PASTEL_MINT",
            "fg_color": "PASTEL_PURPLE",
            "separator": "\uE0B0",
            "separator_fg": "PASTEL_MINT",
            "separator_bg": "RESET"
        }
