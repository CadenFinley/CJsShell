{
  "name": "cjsh-language-support",
  "displayName": "CJsShell Theme Language",
  "description": "Syntax highlighting and basic language support for .cjsh theme files.",
  "version": "0.1.0",
  "publisher": "local",
  "engines": {
    "vscode": "^1.90.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "cjsh-theme",
        "aliases": ["CJsShell Theme", "cjsh"],
        "extensions": [".cjsh"],
        "filenames": [".cjshrc", ".cjsh_logout", ".cjprofile"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cjsh-theme",
        "scopeName": "source.cjsh",
        "path": "./syntaxes/cjsh.tmLanguage.json"
      }
    ]
  }
}
